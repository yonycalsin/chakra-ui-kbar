{
  "name": "@chakra-ui-kbar/core",
  "version": "1.0.0",
  "author": "Yony Calsin",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**",
    "src/"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react @chakra-ui/react @emotion/react @emotion/styled framer-motion kbar",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "clean": "rimraf -rf .turbo dist",
    "clean:force": "pnpm run clean && rimraf -rf node_modules",
    "lint:ts": "tsc --noEmit"
  },
  "devDependencies": {
    "@chakra-ui-kbar/tsconfig": "*",
    "@chakra-ui/react": "2.3.2",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint": "^8.15.0",
    "eslint-config-chakra-ui-kbar": "*",
    "framer-motion": "7.3.5",
    "kbar": "0.1.0-beta.36",
    "react": "^18.1.0",
    "rimraf": "3.0.2",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@chakra-ui/react": "2.3.2",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "framer-motion": "7.3.5",
    "kbar": "0.1.0-beta.36"
  }
}
