{
  "name": "chakra-ui-kbar",
  "author": "Yony Calsin",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "lint": "turbo run lint",
    "clean": "echo \"Hi\"",
    "clean:force": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write --ignore-path .gitignore \"**/*.{ts,tsx,md,js,json}\"",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=docs^... && changeset publish",
    "lint:ts": "turbo run lint:ts --no-cache --parallel --continue"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@yonycalsin/prettier-config-react-typescript": "1.0.1",
    "eslint": "^8.15.0",
    "eslint-config-chakra-ui-kbar": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.11.0",
  "engines": {
    "node": "16.14.0"
  },
  "volta": {
    "node": "16.14.0"
  }
}
